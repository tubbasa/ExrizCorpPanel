
@model ExrizCorpPanel.UI.Models.CustomModels.LayoutModel


@{

    var dd = Model;
    var a = 1;
}

<header class="header-wrapper clearfix">
    <!-- row -->
    <div class="row">
        <!-- col-xs-12 -->
        <div class="col-xs-12">
            <!-- Logo -->
            <div class="logo">
                <a title="Decor" href="index-2.html">
                    <img src="~/@Model.siteDetail.LogoUrl" width="170" height="100" alt="Deccor">
                </a>
            </div>
            <!-- /Logo -->
            <!-- Top Social Icon -->
            <div class="social-icon hidden-xs clearfix">
                <ul>
                    @foreach (var item in Model.socialMedia)
                    {
                        switch (item.TypeName)
                        {
                            case "facebook":
                                <li>
                                    <!-- facebook -->
                                    <a href="@item.Smprofile" title="Facebook" target="_blank"><i class="fa fa-facebook"> </i></a>
                                    <!-- /facebook -->
                                </li>
                                break;
                            case "twitter":
                                <li>
                                    <!-- twitter -->
                                    <a href="@item.Smprofile" title="Twitter" target="_blank"><i class="fa fa-twitter"></i></a>
                                    <!-- /twitter -->
                                </li>
                                break;
                            case "instagram":
                                <li>
                                    <!-- google-plus -->
                                    <a href="@item.Smprofile" title="Google plus" target="_blank"><i class="fa fa-instagram"></i></a>
                                    <!-- /google-plus -->
                                </li>
                                break;
                            case "google":
                                <li>
                                    <!-- google-plus -->
                                    <a href="@item.Smprofile" title="Google plus" target="_blank"><i class="fa fa-google-plus"></i></a>
                                    <!-- /google-plus -->
                                </li>

                                break;

                            case "youtube":
                                <li>
                                    <!-- youtube -->
                                    <a href="@item.Smprofile" title="YouTube" target="_blank"><i class="fa fa-youtube"></i></a>
                                    <!-- /youtube-play -->
                                </li>

                                break;

                            case "linkedin":
                                <li>
                                    <!-- linkedin -->
                                    <a href="@item.Smprofile" title="Linkedin" target="_blank"><i class="fa fa-linkedin"></i></a>
                                    <!-- /linkedin -->
                                </li>
                                break;
                            case "flickr":
                                <li>
                                    <!-- flickr -->
                                    <a href="@item.Smprofile" title="Flickr" target="_blank"><i class="fa fa-flickr"></i></a>
                                    <!-- /flickr -->
                                </li>
                                break;

                        }
                    }
                </ul>
            </div>
            <!-- end of Top Social Icon -->
            <!-- =========================================
            Menu
            ========================================== -->
            <!-- navbar -->
            <div class="navbar navbar-default mainnav">
                <!-- navbar-header -->
                <div class="navbar-header navbar-right pull-right">
                    <!-- offcanvas-trigger-effects -->
                    <div id="offcanvas-trigger-effects" class="column">
                        <button type="button" class="navbar-toggle visible-sm visible-xs" data-toggle="offcanvas"
                                data-target=".navbar-collapse" data-placement="right" data-effect="offcanvas-effect">
                            <i class="fa fa-bars"></i>
                        </button>
                    </div>
                    <!-- offcanvas-trigger-effects -->
                </div>
                <!-- /navbar-header -->
                <!-- navbar-collapse -->
                <div class="collapse navbar-collapse">
                    <!-- navbar-nav -->
                    <ul class="nav navbar-nav navbar-right">
                        <!-- Home Style -->
                        @foreach (var item in Model.menu)
                        {
                            <li class="dropdown">
                                <a href="@item.Url">
                                    @item.Title @if (item.RelatedMenu != null && item.RelatedMenu.Count > 0)
                                    {<b class="caret"></b>}
                                </a>
                                <!-- submenu-wrapper -->
                                @if (item.RelatedMenu != null && item.RelatedMenu.Count > 0)
                                {
                                    <div class="submenu-wrapper submenu-wrapper-topbottom">
                                        <div class="submenu-inner  submenu-inner-topbottom">
                                            <ul class="level1 dropdown-menu">
                                                @foreach (var second in item.RelatedMenu)
                                                {
                                                    <li>
                                                        <a href="@second.Url">
                                                            @second.Title
                                                            @if (second.RelatedMenu != null && second.RelatedMenu.Count > 0)
                                                            {<b class="caret"></b>}
                                                        </a>
                                                    </li>
                                                    @if (second.RelatedMenu != null && second.RelatedMenu.Count > 0)
                                                    {
                                                        <div class="submenu-wrapper submenu-wrapper-topbottom">
                                                            <div class="submenu-inner  submenu-inner-topbottom">
                                                                <ul class="level1 dropdown-menu">
                                                                    @foreach (var third in second.RelatedMenu)
                                                                    {
                                                                        <li>
                                                                            <a href="@third.Url">
                                                                                @third.Title
                                                                                @if (third.RelatedMenu != null && third.RelatedMenu.Count > 0)
                                                                                {<b class="caret"></b>}
                                                                            </a>
                                                                        </li>
                                                                        @if (third.RelatedMenu != null && third.RelatedMenu.Count > 0)
                                                                        {
                                                                            <div class="submenu-wrapper submenu-wrapper-topbottom">
                                                                                <div class="submenu-inner  submenu-inner-topbottom">
                                                                                    <ul class="level1 dropdown-menu">
                                                                                        @foreach (var fourth in third.RelatedMenu)
                                                                                        {
                                                                                            <li>
                                                                                                <a href="@fourth.Url">
                                                                                                    @fourth.Title
                                                                                                    @if (fourth.RelatedMenu != null && fourth.RelatedMenu.Count > 0)
                                                                                                    {<b class="caret"></b>}
                                                                                                </a>
                                                                                            </li>

                                                                                        }
                                                                                    </ul>
                                                                                </div>
                                                                            </div>
                                                                        }

                                                                    }
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    }

                                                }
                                            </ul>
                                        </div>
                                    </div>
                                }
                                <!-- /submenu-wrapper -->
                            </li>
                        }
                        <!-- /Home Style -->



                    </ul>
                    <!-- /navbar-nav -->
                </div>
                <!-- /navbar-collapse -->
            </div>
            <!-- /navbar -->
            <!--End of the menu-->
        </div>
        <!-- /col-xs-12 -->
    </div>
    <!-- /row -->
</header>

<div class="offcanvas-menu offcanvas-effect">
    <button type="button" class="close" aria-hidden="true" data-toggle="offcanvas"
            id="off-canvas-close-btn">
        &times;
    </button>
    <ul>
        @foreach (var item in Model.menu)
        {
            <li>
                <a href="@item.Url">
                    @item.Title @if (item.RelatedMenu != null && item.RelatedMenu.Count > 0)
                    {<b class="caret"></b>}
                </a>
                <!-- submenu-wrapper -->
                @if (item.RelatedMenu != null && item.RelatedMenu.Count > 0)
                {
                            <ul>
                                @foreach (var second in item.RelatedMenu)
                                {
                                    <li>
                                        <a href="@second.Url">
                                            @second.Title
                                            @if (second.RelatedMenu != null && second.RelatedMenu.Count > 0)
                                            {<b class="caret"></b>}
                                        </a>
                                    </li>
                                    @if (second.RelatedMenu != null && second.RelatedMenu.Count > 0)
                                    {
                                                <ul>
                                                    @foreach (var third in second.RelatedMenu)
                                                    {
                                                        <li>
                                                            <a href="@third.Url">
                                                                @third.Title
                                                                @if (third.RelatedMenu != null && third.RelatedMenu.Count > 0)
                                                                {<b class="caret"></b>}
                                                            </a>
                                                        </li>
                                                        @if (third.RelatedMenu != null && third.RelatedMenu.Count > 0)
                                                        {
                                                                    <ul>
                                                                        @foreach (var fourth in third.RelatedMenu)
                                                                        {
                                                                            <li>
                                                                                <a href="@fourth.Url">
                                                                                    @fourth.Title
                                                                                    @if (fourth.RelatedMenu != null && fourth.RelatedMenu.Count > 0)
                                                                                    {<b class="caret"></b>}
                                                                                </a>
                                                                            </li>

                                                                        }
                                                                    </ul>
                                                             
                                                        }

                                                    }
                                                </ul>
                                            
                                    }

                                }
                            </ul>
                       
                }
                <!-- /submenu-wrapper -->
            </li>
        }
      

        </ul>
    </div>
